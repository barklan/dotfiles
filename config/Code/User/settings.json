{
    "vscode-neovim.compositeKeys": {
        "kj": {
            "command": "vscode-neovim.escape",
        }
    },
    "editor.renderWhitespace": "none",
    "editor.acceptSuggestionOnEnter": "off",
    "workbench.enableExperiments": false,
    "window.enableMenuBarMnemonics": false,
    "terminal.integrated.tabs.enabled": false,
    "vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
    "editor.quickSuggestions": {
        "comments": "on",
        "strings": "on",
        "other": "on"
    },
    "[git-commit]": {
        "workbench.editor.restoreViewState": false
    },
    "editor.fontLigatures": true,
    "editor.fontFamily": "'Iosevka', 'Fira Code Retina', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "security.workspace.trust.untrustedFiles": "open",
    "python.formatting.provider": "black",
    "[go]": {
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.tabSize": 4
    },
    "task.problemMatchers.neverPrompt": {
        "shell": true
    },
    "editor.wordWrapColumn": 90,
    "workbench.list.automaticKeyboardNavigation": false,
    "terminal.integrated.commandsToSkipShell": [
        "workbench.action.toggleSidebarVisibility",
        "workbench.actions.view.problems",
        "workbench.action.nextEditor",
        "workbench.action.PreviousEditor",
        "workbench.view.explorer",
        "workbench.action.terminal.openNativeConsole",
        "workbench.action.terminal.goToRecentDirectory",
        "notifications.clearAll"
    ],
    "workbench.startupEditor": "none",
    "editor.lightbulb.enabled": "off",
    "workbench.editor.limit.enabled": true,
    "editor.suggest.localityBonus": true,
    "editor.minimap.renderCharacters": false,
    "editor.inlineSuggest.enabled": true,
    "files.trimTrailingWhitespace": true,
    "files.trimFinalNewlines": true,
    "workbench.editor.limit.perEditorGroup": true,
    "redhat.telemetry.enabled": false,
    "editor.suggest.preview": true,
    "workbench.editor.closeOnFileDelete": true,
    "notebook.consolidatedRunButton": true,
    "terminal.integrated.showExitAlert": false,
    "files.watcherExclude": {
        "**/.cache/**": true,
        "**/vendor/*/**": true,
        "**/.bloop": true,
        "**/.metals": true,
        "**/.ammonite": true
    },
    "search.exclude": {
        "**/__pypackages__/**": true,
        "**/.cache/**": true,
        "**/.pnpm": true,
        "**/dist/**": true,
        "**/go.sum": true,
        "**/out/**": true,
        "**/package-lock.json": true,
        "**/pnpm-lock.yaml": true,
        "**/tmp": true,
        "**/vendor": true
    },
    "go.useLanguageServer": true,
    "gopls": {
        "ui.navigation.symbolScope": "workspace",
        "ui.diagnostic.staticcheck": true,
        "uinavigation.symbolMatcher": "Fuzzy",
        "ui.completion.matcher": "Fuzzy",
        "ui.semanticTokens": true,
        "usePlaceholders": false,
        "formatting.gofumpt": true,
        "analyses": {
            "shadow": false,
            "unusedparams": true,
            "nilness": true,
            "unusedwrite": true
        }
    },
    "task.autoDetect": "off",
    "security.workspace.trust.enabled": false,
    "git.enableSmartCommit": true,
    "git.enableCommitSigning": true,
    "git.showUnpublishedCommitsButton": "never",
    "go.editorContextMenuCommands": {
        "toggleTestFile": false,
        "addTags": false,
        "testAtCursor": false,
        "generateTestForFunction": false,
        "addImport": false,
        "testCoverage": false,
        "playground": false,
        "debugTestAtCursor": false
    },
    "window.restoreFullscreen": true,
    "notebook.cellToolbarLocation": {
        "default": "right"
    },
    "notebook.insertToolbarLocation": "hidden",
    "window.customMenuBarAltFocus": false,
    "jupyter.askForKernelRestart": false,
    "jupyter.showOutlineButtonInNotebookToolbar": false,
    "terminal.integrated.fontFamily": "'Iosevka Term'",
    "editor.find.addExtraSpaceOnTop": false,
    "extensions.ignoreRecommendations": true,
    "editor.scrollbar.verticalScrollbarSize": 10,
    "window.zoomLevel": 0.4,
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.tabs.location": "left",
    "terminal.integrated.tabs.hideCondition": "never",
    "editor.unicodeHighlight.allowedLocales": {
        "ru": true
    },
    "telemetry.telemetryLevel": "off",
    "python.terminal.activateEnvironment": false,
    "editor.semanticHighlighting.enabled": true,
    "editor.accessibilitySupport": "off",
    "notebook.breadcrumbs.showCodeCells": false,
    "go.lintFlags": [
        "--fast"
    ],
    "go.lintTool": "golangci-lint",
    "go.addTags": {
        "tags": "json",
        "options": "json=omitempty",
        "promptForTags": false,
        "transform": "snakecase",
        "template": ""
    },
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1,
        "vscodevim.vim": 1
    },
    "breadcrumbs.symbolPath": "last",
    "files.autoSave": "afterDelay",
    "notebook.globalToolbar": false,
    "notebook.showCellStatusBar": "hidden",
    "notebook.lineNumbers": "on",
    "editor.wordBasedSuggestionsMode": "allDocuments",
    "explorer.fileNesting.enabled": false,
    "terminal.integrated.tabs.showActions": "never",
    "terminal.integrated.tabs.showActiveTerminal": "never",
    "vscode-neovim.useCtrlKeysForNormalMode": false,
    "workbench.editor.limit.value": 5,
    "editor.smoothScrolling": true,
    "editor.tabCompletion": "on",
    "editor.bracketPairColorization.enabled": false,
    "notebook.markup.fontSize": 14,
    "debug.disassemblyView.showSourceCode": false,
    "notebook.showFoldingControls": "never",
    "files.insertFinalNewline": true,
    "go.inlayHints.compositeLiteralFields": true,
    "go.inlayHints.parameterNames": true,
    "go.inlayHints.assignVariableTypes": true,
    "workbench.localHistory.enabled": false,
    "go.inlayHints.rangeVariableTypes": true,
    "editor.guides.bracketPairsHorizontal": false,
    "workbench.tree.renderIndentGuides": "always",
    "git.openRepositoryInParentFolders": "never",
    "debug.console.fontSize": 12,
    "editor.cursorBlinking": "solid",
    "go.languageServerFlags": [
        "-remote=auto",
        "-remote.listen.timeout=15m"
    ],
    "editor.stickyScroll.maxLineCount": 3,
    "editor.stickyScroll.enabled": true,
    "window.dialogStyle": "custom",
    "workbench.editor.enablePreview": false,
    "editor.wordWrap": "on",
    "workbench.editor.empty.hint": "hidden",
    "workbench.activityBar.location": "top",
    "window.menuBarVisibility": "hidden",
    "explorer.compactFolders": false,
    "vscode-neovim.ctrlKeysForInsertMode": [
        "a",
        "d",
        "h",
        "j",
        "o",
        "r",
        "t",
        "u",
        "e"
    ],
    "window.commandCenter": false,
    "workbench.productIconTheme": "icons-carbon",
    "window.title": "${rootName}${separator}VSC",
    "vscode-neovim.ctrlKeysForNormalMode": [
        "a",
        "b",
        "c",
        "d",
        "e",
        "f",
        "h",
        "i",
        "k",
        "l",
        "o",
        "r",
        "t",
        "u",
        "v",
        "x",
        "y",
        "z",
        "/",
        "]",
        "right",
        "left",
        "up",
        "down",
        "backspace",
        "delete"
    ],
    "hexeditor.columnWidth": 20,
    "hexeditor.showDecodedText": true,
    "hexeditor.defaultEndianness": "little",
    "hexeditor.inspectorType": "aside",
    "go.inlayHints.constantValues": true,
    "go.survey.prompt": false,
    "editor.inlayHints.fontFamily": "Iosevka Light",
    "editor.inlayHints.fontSize": 13.5,
    "editor.minimap.autohide": true,
    "editor.showFoldingControls": "never",
    "workbench.quickOpen.preserveInput": true,
    "search.quickOpen.history.filterSortOrder": "recency",
    "search.quickOpen.includeHistory": false,
    "search.smartCase": true,
    "git.branchProtection": [
        "release"
    ],
    "git.enableStatusBarSync": false,
    "git.showPushSuccessNotification": true,
    "terminal.integrated.shellIntegration.enabled": false,
    "terminal.explorerKind": "external",
    "workbench.editor.tabActionCloseVisibility": false,
    "editor.matchBrackets": "never",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.tree.indent": 14,
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.layoutControl.enabled": false,
    "window.customTitleBarVisibility": "never",
    "window.titleBarStyle": "native",
    "workbench.colorTheme": "Tokyo Night Light",
    "window.zoomPerWindow": false
}
